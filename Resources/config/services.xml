<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="tadcka_text.manager.class">Tadcka\TextBundle\Manager\TextManager</parameter>
        <parameter key="tadcka_text.administrator_manager.class">Tadcka\TextBundle\Manager\TextAdministratorManager</parameter>
    </parameters>

    <services>
        <service id="tadcka_text.manager" class="%tadcka_text.manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="tadcka_text.administrator_manager" class="%tadcka_text.administrator_manager.class%">
            <argument type="service" id="doctrine" />
        </service>
    </services>
</container>
